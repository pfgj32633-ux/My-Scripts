local function rollChance()
    local roll = math.random(1,100) -- random tá»« 1 tá»›i 100
    
    if roll == 1 then -- 1% tá»‰ lá»‡
        print("TRÃšNG 1% >:D kÃ­ch hoáº¡t script")

        --====== Load Spawner ======--
loadstring = _G.loadstring

local spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/VoorPhale012/Fogroom/refs/heads/main/Wwwhyyyyil"))()

--====== Create Entity ======--
local entity = spawner.Create({
	Entity = {
		Name = "A-60",
		Asset = "https://github.com/vuivuiviu2/Pressure-/raw/main/a-600.rbxm",
		HeightOffset = 0
	},
	Lights = {  -- nÃƒ o 
		Flicker = {
			Enabled = false,
			Duration = 0.8
		},
		Shatter = false,
		Repair = false
	},
	Earthquake = { Enabled = false },
	CameraShake = { Enabled = true, Range = 100, Values = {1.5, 20, 0.1, 1} },
	Movement = { Speed = 200, Delay = 4, Reversed = false },
	Rebounding = { Enabled = true, Type = "Blitz", Min = 1, Max = 1, Delay = 0 },
	Damage = { Enabled = true, Range = 50, Amount = 66 },
	Crucifixion = { Enabled = true, Range = 50, Resist = false, Break = true },
	Death = { Type = "Default", Hints = {"Lol", "LOL", "LOLLLLLLLLLL"}, Cause = "A-60 The Multi Monster" }
})

--====== Callbacks ======--
entity:SetCallback("OnSpawned", function()
	print("A-60 has spawned")
	local Player = game.Players.LocalPlayer
	if not Player then return end
	local PlayerGui = Player:WaitForChild("PlayerGui")

	-- ===== Static nhÃ¡ÂºÂ¥p nhÃƒÂ¡y =====
	local staticGui = Instance.new("ScreenGui")
	staticGui.Name = "A60Static"
	staticGui.IgnoreGuiInset = true
	staticGui.ResetOnSpawn = false
	staticGui.Parent = PlayerGui

	local staticImg = Instance.new("ImageLabel")
	staticImg.Size = UDim2.new(1,0,1,0)
	staticImg.Position = UDim2.new(0,0,0,0)
	staticImg.BackgroundTransparency = 1
	staticImg.Image = "rbxassetid://120865463688240" -- texture static mÃ¡Â»â€ºi
	staticImg.ImageTransparency = 0.95
	staticImg.ZIndex = 50
	staticImg.Parent = staticGui

	-- NhÃ¡ÂºÂ¥p nhÃƒÂ¡y nhÃ¡ÂºÂ¹
	local running = true
	spawn(function()
		while running do
			if staticImg then
				staticImg.ImageTransparency = 0.93 + math.random() * 0.2
			end
			task.wait(0.1)
		end
	end)

	-- XÃƒÂ³a static sau 5 giÃƒÂ¢y nÃ¡ÂºÂ¿u chÃ†Â°a despawn
	task.delay(5, function()
		running = false
		if staticGui then staticGui:Destroy() end
	end)

	-- ===== OnSpawned gÃ¡Â»â€˜c (text label) =====
	task.delay(4, function()
		local ScreenGui = Instance.new("ScreenGui")
		ScreenGui.Name = "A60Message"
		ScreenGui.IgnoreGuiInset = true
		ScreenGui.ResetOnSpawn = false
		ScreenGui.Parent = PlayerGui

		local label = Instance.new("TextLabel")
		label.Size = UDim2.new(0, 400, 0, 50)
		label.Position = UDim2.new(0.5, -200, 0.5, 50)
		label.BackgroundTransparency = 1
		label.TextScaled = false
		label.TextSize = 26
		label.Font = Enum.Font.Oswald
		label.TextColor3 = Color3.fromRGB(255, 64, 64)
		label.TextStrokeTransparency = 0.2
		label.TextStrokeColor3 = Color3.new(0, 0, 0)
		label.TextWrapped = true
		label.Text = ({
			"coming through.",
			"heads up.",
			"incoming.",
			"ready?",
			"think fast."
		})[math.random(1, 5)]
		label.Parent = ScreenGui

		task.delay(2.5, function()
			if ScreenGui then ScreenGui:Destroy() end
		end)
	end)
end)

entity:SetCallback("OnStartMoving", function() print("A-60 has started moving") end)
entity:SetCallback("OnRebounding", function(start)
	if start then print("A-60 is rebounding") else print("A-60 finished rebound") end
end)

entity:SetCallback("OnDespawned", function()
	print("A-60 has despawned")
	local Player = game.Players.LocalPlayer
	if Player then
		local PlayerGui = Player:FindFirstChild("PlayerGui")
		if PlayerGui then
			local static = PlayerGui:FindFirstChild("A60Static")
			if static then static:Destroy() end
		end
	end
end)

entity:SetCallback("OnDamagePlayer", function(newHealth)
	local Player = game.Players.LocalPlayer
	if not Player then return end

-- ==============================
-- Ã„ÂÃ¡Â»â€I VÃ¡ÂºÂ¬T LIÃ¡Â»â€ U + MÃƒâ‚¬U CÃ¡Â»Â¬A
-- ==============================
task.spawn(function()

    local DoorMaterial = Enum.Material.DiamondPlate
    local DoorColor    = Color3.fromRGB(55, 55, 55)

    local function ConvertRoom(Room)
        task.wait(1.5)
        if not Room:IsA("Model") then return end

        for _, obj in pairs(Room:GetDescendants()) do
            if obj:IsA("BasePart") and obj.Name == "Door" then
                obj.Material = DoorMaterial
                obj.Color = DoorColor

                for _, child in pairs(obj:GetDescendants()) do
                    if child:IsA("BasePart") then
                        child.Material = DoorMaterial
                        child.Color = DoorColor
                    end
                end
            end
        end
    end

    workspace.CurrentRooms.ChildAdded:Connect(ConvertRoom)

    for _, room in pairs(workspace.CurrentRooms:GetChildren()) do
        ConvertRoom(room)
    end

end)

-- =====================================
-- Ã„ÂÃ¡Â»â€I TEXT CÃ¡Â»Â¬A Ã¢â€ â€™ ":)" (GIÃ¡Â»Â® KHUNG)
-- =====================================
task.spawn(function()

    local GameData = game:GetService("ReplicatedStorage"):WaitForChild("GameData")
    local Rooms = workspace:WaitForChild("CurrentRooms")
    local CUSTOM_FONT = Enum.Font.Oswald

    local function editDoorText(room)
        if not room then return end

        for _, ui in pairs(room:GetDescendants()) do
            if (ui:IsA("TextLabel") or ui:IsA("TextButton")) and ui.Text then
                
                if not ui:GetAttribute("CustomDoorTag") then
                    
                    ui.Text = ":)"
                    ui.TextColor3 = Color3.fromRGB(255, 0, 0)
                    ui.Font = CUSTOM_FONT
                    ui.TextScaled = true
                    
                    ui:SetAttribute("CustomDoorTag", true)
                end
            end
        end
    end

    GameData.LatestRoom.Changed:Connect(function()
        task.wait(0.2)
        local room = Rooms:FindFirstChild(GameData.LatestRoom.Value)
        editDoorText(room)
    end)

    task.wait(1)
    editDoorText(Rooms:FindFirstChild(GameData.LatestRoom.Value))

end)

-- MischeviousLight RED Edition
-- Modified for Full Red Theme

_G.DO_RED_GUY = true 

local Players = game:GetService("Players")
local SoundService = game:GetService("SoundService")

local Camera = workspace.CurrentCamera
local DeathName = "DeathBackgroundBlue"

-- Ã„ÂÃ¡Â»â€¢i sequence sang mÃƒ u Ã„â€˜Ã¡Â»Â rÃ¡Â»Â±c
local RedPrimary = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 0, 0)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(150, 0, 0))
})

local Player = Players.LocalPlayer
local PlayerGui = Player.PlayerGui
local MainUI = PlayerGui.MainUI
local Death = MainUI.Death
local HelpfulDialogue = Death.HelpfulDialogue
local MainGame = MainUI.Initiator.Main_Game
local Health = MainGame.Health
local Music = Health.Music

local function getGitSoundId(GithubSoundPath: string, AssetName: string): Sound
    local Url = GithubSoundPath

    if not isfile(AssetName..".mp3") then 
        writefile(AssetName..".mp3", game:HttpGet(Url)) 
    end

    local Sound = Instance.new("Sound")
    Sound.SoundId = getcustomasset(AssetName..".mp3", true)
    return Sound 
end

Camera.ChildAdded:Connect(
    function(Child): ()
        if Child.Name ~= DeathName then
            return
        end

        if not _G.DO_RED_GUY then
            return
        end

        for _, Asset in Music:GetChildren() do
            if Asset:IsA("Sound") then
                Asset:Destroy() 
            end
        end

        local Lights = Child.Lights
        local Fog = Child.FogAndSmaller
        local Water = Child.Water

        -- Ã„ÂÃ¡Â»â€¢i ÃƒÂ¡nh sÃƒÂ¡ng Ã„â€˜ÃƒÂ¨n Spotlight sang mÃƒ u Ã„â€˜Ã¡Â»Â
        for _, Light in Lights:GetDescendants() do
            if Light:IsA("SpotLight") then
                Light.Color = Color3.fromRGB(255, 0, 0)
            end

            if Light:IsA("ParticleEmitter") then
                Light.Color = RedPrimary
            end
        end

        -- Ã„ÂÃ¡Â»â€¢i mÃƒ u sÃ†Â°Ã†Â¡ng mÃƒÂ¹ sang Ã„â€˜Ã¡Â»Â
        for _, FogL in Fog:GetChildren() do
            FogL.Color = RedPrimary
        end

        -- Ã„ÂÃ¡Â»â€¢i mÃƒ u nÃ†Â°Ã¡Â»â€ºc sang Ã„â€˜Ã¡Â»Â
        for _, WaterL in Water:GetChildren() do
            WaterL.Color = RedPrimary
        end

        local BigLight = Lights.BigLight
        local Attachment = BigLight.Attachment
        local Moon = Attachment.Moon
        
        -- Thay Ã„â€˜Ã¡Â»â€¢i ID hÃƒÂ¬nh Ã¡ÂºÂ£nh theo yÃƒÂªu cÃ¡ÂºÂ§u
        Moon.Texture = "rbxassetid://95704191460526"
        
        -- ChÃ¡Â»â€°nh mÃƒ u chÃ¡Â»Â¯ hÃ†Â°Ã¡Â»â€ºng dÃ¡ÂºÂ«n thÃƒ nh Ã„â€˜Ã¡Â»Â hoÃ¡ÂºÂ·c trÃ¡ÂºÂ¯ng tÃƒÂ¹y bÃ¡ÂºÂ¡n (Ã„â€˜ang Ã„â€˜Ã¡Â»Æ’ Ã„â€˜Ã¡Â»Â cho Ã„â€˜Ã¡Â»â€œng bÃ¡Â»â„¢)
        HelpfulDialogue.TextColor3 = Color3.fromRGB(255, 0, 0)

        local CustomMusic = getGitSoundId("https://github.com/vuivuiviu2/Vault-Mode/raw/refs/heads/main/cata_1.mp3", "Darkness")
        CustomMusic.Parent = SoundService
        CustomMusic.Looped = true
        CustomMusic.Volume = 1.5
        CustomMusic:Play()
    end
)


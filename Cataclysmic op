local lighting = game:GetService("Lighting")

lighting.FogColor = Color3.fromRGB(30, 0, 0)
lighting.FogStart = 0
lighting.FogEnd = 0

task.spawn(function()
    for i = 0, 30 do
        lighting.FogStart = i
        task.wait(0.05)
    end
end)

task.spawn(function()
    for i = 0, 95 do
        lighting.FogEnd = i
        task.wait(0.05)
    end
end)

local mainGameModule = game.Players.LocalPlayer.PlayerGui
    :WaitForChild("MainUI", 10)
    :WaitForChild("Initiator", 10)
    :FindFirstChild("Main_Game")

local chatService = game:GetService("TextChatService")
local channel = chatService.TextChannels:FindFirstChild("RBXGeneral")

local function showCaptionAndChat(captionText, chatText)
    if mainGameModule then
        local success, mainGame = pcall(require, mainGameModule)
        if success and mainGame and mainGame.caption then
            mainGame.caption("<font color='#ff0000'>"..captionText.."</font>", true)
        end
    end
    if channel then
        channel:DisplaySystemMessage("<font color='#ff0000'>"..chatText.."</font>")
    end
end

showCaptionAndChat("Cataclysmic Mode Loaded Successfully.", "Cata Light: Thanks for playing Cataclysmic Mode")
task.wait(5)

showCaptionAndChat("Mode made by Vuivuiviu2 and Guestly", "Cata Light: Mode made by Vuivuiviu2 and Guestly")
task.wait(5)

showCaptionAndChat("Have fun ðŸ‘", "Good Luck")

